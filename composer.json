{
    "name": "opendaje/identity-access",
    "type": "library",
    "description": "Identity Access library",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Zerai Teclai",
            "email": "teclaizerai@google.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "doctrine/collections": "^1.5",
        "psr/log": "^1.1",
        "ramsey/uuid": "^3.8"
    },
    "require-dev": {
        "bamarni/composer-bin-plugin": "^1.4",
        "behat/behat": "^3.5",
        "phpunit/phpunit": "^7",
        "vimeo/psalm": "^3.8"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "bamarni-bin": {
            "target-directory": "tools"
        }
    },
    "autoload": {
        "psr-4": {
            "OpenDaje\\IdentityAccess\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OpenDaje\\IdentityAccess\\Tests\\": "tests"
        }
    },
    "minimum-stability": "stable"
}
