{
    "name": "opendaje/identity-access",
    "description": "Identity Access library",
    "type": "project",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Zerai Teclai",
            "email": "teclaizerai.com@googlemail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.2",
        "doctrine/collections": "^1.5",
        "psr/log": "^1.1",
        "ramsey/uuid": "^3.8"
    },
    "require-dev": {
        "behat/behat": "^3.5",
        "friendsofphp/php-cs-fixer": "^2.14",
        "phpstan/phpstan": "^0.12",
        "phpunit/phpunit": "^7",
        "prolic/fpp": "dev-master",
        "vimeo/psalm": "^3.8"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "OpenDaje\\IdentityAccess\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OpenDaje\\IdentityAccess\\Tests\\": "tests"
        }
    }
}
